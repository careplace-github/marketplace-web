{
  "name": "careplace-marketplace",
  "author": "Careplace",
  "version": "0.1.29",
  "description": "Careplace - Marketplace de Cuidadores Domiciliários. A Careplace conecta Empresas de que prestam Serviços de Apoio Domiciliário às pessoas que precisam de encontrar um Cuidador Domiciliário.",
  "private": true,
  "scripts": {
    "start": "ENV=prod next start -p 3000",
    "start:staging": "ENV=stag next start -p 3000",
    "start:development": "ENV=dev next start -p 3000",
    "build": "ENV=prod next build",
    "build:development": "ENV=dev next build",
    "build:staging": "ENV=stag next build",
    "dev": "ENV=dev next dev -p 3000",
    "dev:staging": "ENV=stag next dev -p 3000",
    "build-netlify": "next build && cp -r .next _next && mv _next .next/",
    "export": "yarn build && next export -o _static",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "test": "jest",
    "test:snyk": "snyk test",
    "coverage": "jest --coverage",
    "e2e": "cypress open",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier-check": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "clear-all": "rm -rf .next _static node_modules",
    "re-start": "rm -rf .next _static node_modules && yarn install && yarn dev",
    "re-build": "rm -rf .next _static node_modules && yarn install && yarn build",
    "dependency": "dependency-cruiser ./src --format html --output-type html --output-dir ./report",
    "depcheck": "depcheck --config=.depcheckrc"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.0.0",
    "@iconify/react": "^4.1.0",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.14.14",
    "@mui/system": "^5.11.15",
    "@mui/x-date-pickers": "^6.0.3",
    "@react-google-maps/api": "^2.18.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@stripe/react-stripe-js": "^2.2.0",
    "@stripe/stripe-js": "^2.1.2",
    "@types/lodash": "^4.14.192",
    "@types/redux": "^3.6.0",
    "axios": "^1.3.5",
    "cypress": "12.17.4",
    "date-fns": "^2.29.3",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.11.6",
    "google-maps": "^4.3.3",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "jquery": "^1.8.0",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "mui-tel-input": "^3.2.2",
    "next": "13.2.4",
    "notistack": "^3.0.1",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.2.0",
    "react-lazy-load-image-component": "^1.5.6",
    "react-player": "^2.12.0",
    "react-redux": "^8.0.5",
    "react-scroll": "^1.8.9",
    "react-slick": "^0.29.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "simplebar-react": "^3.2.3",
    "sitemap": "^7.1.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.9",
    "stylis": "^4.1.1",
    "stylis-plugin-rtl": "^2.0.2",
    "use-places-autocomplete": "^4.0.0",
    "web-vitals": "^0.2.4",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "yet-another-react-lightbox": "^3.2.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.3.0",
    "@types/lodash": "^4.14.192",
    "@types/numeral": "^2.0.2",
    "@types/react-dom": "^18.0.11",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "depcheck": "^1.4.5",
    "dependency-cruiser": "^12.12.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "snyk": "^1.1214.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.0.4"
  }
}
